        android:layout_width="98dp"
        android:layout_height="28dp"
        android:layout_marginStart="8dp"
        android:text="Aamu"
        app:layout_constraintEnd_toStartOf="@+id/textViewLaakePaiva"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <TextView
        android:id="@+id/textViewLaakePaiva"
        android:layout_width="92dp"
        android:layout_height="25dp"
        android:text="P채iv채"
        app:layout_constraintEnd_toStartOf="@+id/textViewLaakeIlta"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewLaakeAamu"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <TextView
        android:id="@+id/textViewLaakeIlta"
        android:layout_width="112dp"
        android:layout_height="25dp"
        android:layout_marginEnd="8dp"
        android:text="Ilta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewLaakePaiva"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.20930232" />

    <Switch
        android:id="@+id/laaksewitch_aamut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="muutaAamut"
        android:text="Aamut"
        app:layout_constraintEnd_toStartOf="@+id/laaksewitch_paivat"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewLaakeAamu" />

    <Switch
        android:id="@+id/laakeswitch_aamu_ma"
