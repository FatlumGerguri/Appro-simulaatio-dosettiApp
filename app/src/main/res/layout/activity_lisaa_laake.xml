    <Switch
        android:id="@+id/laakeswitch_paiva_ma"
        android:layout_width="80dp"
        android:layout_height="39dp"
        android:checked="false"
        android:clickable="true"
        android:onClick="switchTheSwitch"
        android:text="Ma"
        app:layout_constraintEnd_toStartOf="@+id/laakeswitch_ilta_ma"
        app:layout_constraintHorizontal_bias="0.5"
